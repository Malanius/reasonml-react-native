{
  "name": "reasonml-training",
  "version": "0.1.0",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": ["reason-react", "reason-react-native", "bs-fetch"],
  "warnings": {
    "error": "+101+8"
  },
  "namespace": true,
  "refmt": 3,
  "reason": {
    "react-jsx": 3
  },
  "gentypeconfig": {
    "language": "typescript",
    "shims": {},
    "debug": {
      "all": false,
      "basic": false
    }
  }
}
